{
  "name": "ai-sdk-provider-goose-web",
  "version": "0.1.0",
  "description": "AI SDK v5 provider for Goose via WebSocket connection",
  "keywords": [
    "ai-sdk",
    "goose",
    "websocket",
    "language-model",
    "llm"
  ],
  "homepage": "https://github.com/evanfeenstra/ai-sdk-provider-goose-web",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/evanfeenstra/ai-sdk-provider-goose-web.git"
  },
  "bugs": {
    "url": "https://github.com/evanfeenstra/ai-sdk-provider-goose-web/issues"
  },
  "license": "MIT",
  "author": "Evan Feenstra",
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf dist",
    "dev": "tsup --watch",
    "lint": "eslint src",
    "prepare": "npm run build",
    "prepublishOnly": "npm run clean && npm run build",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "example": "npm run build && npx tsx example-usage.ts",
    "example:dev": "npx tsx example-usage.ts",
    "debug": "npm run build && npx tsx debug-streaming.ts"
  },
  "dependencies": {
    "@ai-sdk/provider": "2.0.0",
    "@ai-sdk/provider-utils": "3.0.3",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/node": "20.17.24",
    "@types/ws": "^8.5.12",
    "@typescript-eslint/eslint-plugin": "8.34.0",
    "@typescript-eslint/parser": "8.34.0",
    "ai": "5.0.14",
    "eslint": "9.28.0",
    "tsup": "8.5.0",
    "tsx": "^4.19.2",
    "typescript": "5.6.3",
    "vitest": "3.2.4",
    "zod": "^4.0.0"
  },
  "peerDependencies": {
    "zod": "^4.0.0"
  },
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  }
}
